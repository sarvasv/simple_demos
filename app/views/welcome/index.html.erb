<h1 id="demonstrations">Demonstrations</h1>
<p>
  <ul>
    <!-- # 
    #  Fri Aug 19 23:48:15 IST 2011, ramonrails
    #   * dynamically create links for demonstration pages we have -->
    <% @demos.each do |num| %>
    <li><span class="demo_name">
      <%= link_to (@demo_names[ num].blank? ? "Demo #{num}" : @demo_names[ num]), :controller => "welcome", :action => "demo_#{num}" %>
    </span></li>
    <% end %>
  </ul>
</p>